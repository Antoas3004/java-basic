public class Arithmetic_2 {

	int add(int a, int b) { // parameterized method //local variable
		int c = a + b;
		System.out.println(c);
		return c;
	}

	int multi(int a, int b) { // parameterized method //local variable
		int c = a * b;
		System.out.println(c);
		return c;
	}

	public static void main(String[] args) {
		Arithmetic_2 calc = new Arithmetic_2();
		calc.add(1, 50);
		calc.multi(1, 50);
	}
}
